{
  "name": "eslint-config-rubiconproject",
  "version": "0.5.0",
  "description": "the Rubicon Project's Shareable ESLint Config",
  "main": "index.js",
  "scripts": {
    "precommit": "npm test",
    "presymlink": "rm -f node_modules/eslint-config-rubiconproject",
    "pretest": "npm run symlink",
    "symlink": "ln -s ../ node_modules/eslint-config-rubiconproject",
    "test": "eslint ."
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "promise",
      "standard"
    ],
    "extends": [
      "standard",
      "standard-jsx",
      "rubiconproject"
    ]
  },
  "author": "sterpe@rubiconproject.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sterpe/eslint-config-rubiconproject.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "bugs": {
    "url": "https://github.com/sterpe/eslint-config-rubiconproject/issues"
  },
  "homepage": "https://github.com/sterpe/eslint-config-rubiconproject#readme",
  "dependencies": {
    "eslint-config-standard": "^5.2.0",
    "eslint-config-standard-jsx": "^1.2.0"
  },
  "peerDependencies": {
    "eslint": "^2.8.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2"
  },
  "devDependencies": {
    "eslint": "^2.8.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2"
  }
}
